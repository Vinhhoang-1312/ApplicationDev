
@model StaffTrainee.Models.UserInfo
@{
    ViewBag.Title = "Index";
}

<h2>My User Information</h2>
<div class="row">
    <div class="col-md-4">
        <div class="panel panel-info">
            <div class="panel-body">
                <p>Full Name: @Model.FullName</p>
                <p>Phone: @Model.Phone</p>
            </div>
        </div>
    </div>
</div>

<div class="form-group">
    @Html.ActionLink(
    "Edit Info",
    "Edit",
    null,
    new { @class = "btn btn-info" }
    )
</div>
