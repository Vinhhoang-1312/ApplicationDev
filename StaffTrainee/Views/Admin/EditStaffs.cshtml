@model StaffTrainee.Models.ApplicationUser
@{
    ViewBag.Title = "EditStaffs";
}

<h2>EditStaffs</h2>


@Html.ValidationSummary(false,"", new { @class = "text-danger" })
@using (Html.BeginForm("EditStaffs", "Admin", FormMethod.Post))
{
    @Html.HiddenFor(p => p.Id)
    <div class="form-group">
        @Html.AntiForgeryToken()
        @Html.LabelFor(p => p.Email)
        @Html.EditorFor(p => p.Email, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(p => p.Email, "", new { @class = "text-danger" })
        
            @*@Html.LabelFor(p => p.PhoneNumber)
            @Html.EditorFor(p => p.PhoneNumber, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(p => p.PhoneNumber, "", new { @class = "text-danger" })*@
            
    </div>
    <button type="submit" class="btn btn-primary">Edit</button>
}